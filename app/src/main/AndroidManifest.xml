<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.complains">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DocumentActivity" />
        <activity
            android:name=".activity.AgreementTypeActivity"
            android:label="@string/title_activity_agreement_type"></activity>
        <activity
            android:name=".activity.ProblemActivity"
            android:label="@string/title_activity_problem"></activity>
        <activity
            android:name=".activity.ActionActivity"
            android:label="@string/title_activity_action" />
    </application>

</manifest>
